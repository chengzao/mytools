{
  "extends": "./nostrictnull",
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": false,
    "module": "es2015",
    "sourceMap": true,
    "target": "es6",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "noImplicitThis": true,
    "moduleResolution": "Node",
    "emitDecoratorMetadata": true,
    "declaration": true
  },
 
  "exclude": [
    "node_modules"
  ],
  "include": [
    "./src/**/*"
  ],
}